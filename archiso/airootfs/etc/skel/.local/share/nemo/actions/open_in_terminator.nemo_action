[Nemo Action]

 Name=Tilix
 Comment=Open the 'tilix' terminal in the selected folder
 Exec=tilix --working-directory="%F"
 Icon-Name=terminal
 Selection=any
 Extensions=dir;

 Name=Kitty
 Comment=Open the 'kitty' terminal in the selected folder
 Exec=kitty --working-directory="%F"
 Icon-Name=terminal
 Selection=any
 Extensions=dir;